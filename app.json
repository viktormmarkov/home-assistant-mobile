{
  "expo": {
    "name": "home-assistant-mobile",
    "slug": "home-assistant-mobile",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": { 
      "supportsTablet": true
    },
    "android": {
      "package": "com.vms.homeassistant",
      "versionCode": 4
    }
  }
}
